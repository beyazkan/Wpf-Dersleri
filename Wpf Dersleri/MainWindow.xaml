<Window x:Class="Wpf_Dersleri.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Dersleri"
        mc:Ignorable="d"
        Title="Varsayılan Kurulumlar" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="397*"/>
            <RowDefinition Height="22*"/>
        </Grid.RowDefinitions>

        <ListBox Grid.Row="0">
            <ListBoxItem>Google Chrome Enterprise</ListBoxItem>
            <ListBoxItem>Oracle Database Client 11G</ListBoxItem>
            <ListBoxItem>Hbys Kurulumu</ListBoxItem>
            <ListBoxItem>MsoShutdownService</ListBoxItem>
            <ListBoxItem>FastReport 2010</ListBoxItem>
            <ListBoxItem>AKİS E-imza</ListBoxItem>
            <ListBoxItem>KamuSM E-İmza Uygulaması</ListBoxItem>
            <ListBoxItem>Gemalto Driver</ListBoxItem>
            <ListBoxItem>ACS - ACR Driver</ListBoxItem>
            <ListBoxItem>Microsoft Office</ListBoxItem>
            <ListBoxItem>JAVA</ListBoxItem>
        </ListBox>

        <Button Content="Kur" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Grid.Row="1" Height="20"/>

    </Grid>
</Window>
